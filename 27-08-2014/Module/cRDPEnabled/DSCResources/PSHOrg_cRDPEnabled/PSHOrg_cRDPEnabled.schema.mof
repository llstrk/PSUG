
[ClassVersion("1.0.0.0"), FriendlyName("cRDPEnabled")]
class PSHOrg_cRDPEnabled : OMI_BaseResource
{
	[Key, Description("Set to indicate if RDP for remote administration should be enabled or not")] Boolean Enabled;
